<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="home" prefHeight="400.0" prefWidth="600.0" stylesheets="@../CSS/HomePage.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.HomePageController">
   <children>
      <HBox prefHeight="50.0" prefWidth="600.0" styleClass="topBar">
         <children>
            <Button fx:id="createNewWorkoutButton" mnemonicParsing="false" onAction="#createWorkoutPage" styleClass="topButtons" text="+ Create New Workout">
               <HBox.margin>
                  <Insets left="10.0" top="10.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="signOutButton" mnemonicParsing="false" styleClass="topButtons" text="Sign Out">
               <HBox.margin>
                  <Insets left="378.0" top="10.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
      <Label fx:id="myWorkoutsLabel" layoutX="52.0" layoutY="69.0" text="My Workouts">
         <font>
            <Font name="DengXian Regular" size="21.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
